{
  "name": "mmbn-dialog-previewer",
  "displayName": "MMBN Dialog Previewer",
  "description": "",
  "version": "0.0.1",
  "publisher": "SolidOne",
  "engines": {
    "vscode": "^1.61.0"
  },
  "categories": [
    "Visualization"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "activationEvents": [
    "onCommand:mmbn-dialog-previewer.openview.focus",
    "onView:mmbn-dialog-previewer.openview"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "MMBN Dialog Previewer",
          "icon": "assets/logo_mmbn_previewer.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "mmbn-dialog-previewer.openview",
          "name": "MMBN Dialog Previewer",
          "contextualTitle": "MMBN Dialog Previewer"
        }
      ]
    },
    "configuration": {
      "title": "MMBN Dialog Previewer",
      "properties": {
        "mmbnDialogPreviewer.error.usewarnings": {
          "type": "boolean",
          "default": false,
          "description": "Should take in account warnings"
        },
        "mmbnDialogPreviewer.previewType": {
          "type": "string",
          "default": "mmbn1_dialogues",
          "enum": [
            "mmbn1_dialogues",
            "mmbn1_chips",
            "mmbn2_dialogues",
            "mmbn2_dialogues_vwf",
            "mmbn2_chips",
            "mmbn2_chips_vwf",
            "mmbn3_dialogues",
            "mmbn3_dialogues_vwf",
            "mmbn3_chips",
            "mmbn3_chips_vwf",
            "mmbn4_dialogues",
            "mmbn4_chips"
          ],
          "description": "The chosen preview type"
        }
      }
    },
    "commands": [
      {
        "command": "mmbn-dialog-previewer.openview.focus",
        "category": "MMBN Dialog Previewer",
        "title": "Open Sidebar",
        "icon": "$(clear-all)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build:sass": "sass assets:assets --style=compressed",
    "watch:sass": "sass --watch assets:assets --style=compressed"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.61.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "sass": "^1.89.0",
    "typescript": "^4.4.3"
  }
}
